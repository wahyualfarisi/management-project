<div class="row">
        <div class="page-header">
            <div class="d-flex align-items-center">
                <div>
                    <ul class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#/project"><i class="ti ti-home"></i></a></li>
                        <li class="breadcrumb-item active">create project</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
<div class="widget has-shadow">
    <div class="widget-header bordered no-actions d-flex align-items-center">
        <h4>Create Your Project</h4>
    </div>

    <div class="row flex-row">
        <div class="col-12">
            <div class="widget-body">
                <form id="form-create-project">
                <div class="form-horizontal">
                    <div class="form-group row d-flex align-items-center mb-5">
                        <label class="col-lg-3 form-control-label">Start Day</label>
                        <div class="col-lg-9">
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon">
                                        <i class="la la-calendar"></i>
                                    </span>
                                    <input type="text" class="form-control dateproject" id="start_day" name="start_day"  placeholder="Select value">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-group row d-flex align-items-center mb-5">
                            <label class="col-lg-3 form-control-label">Finish Day</label>
                            <div class="col-lg-9">
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="la la-calendar"></i>
                                        </span>
                                        <input type="text" class="form-control dateproject" id="finish_day" name="finish_day" placeholder="Select value">
                                    </div>
                                </div>
                            </div>
                    </div>

                    <div class="form-group row d-flex align-items-center mb-5">
                            <label class="col-lg-3 form-control-label">Name Of Project</label>
                            <div class="col-lg-9">
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="la la-thumb-tack"></i>
                                        </span>
                                        <input type="text" class="form-control" id="name_project" name="name_project" placeholder="Enter Name Of Project ..">
                                    </div>
                                </div>
                            </div>
                    </div>


                    <div class="form-group row d-flex align-items-center mb-5">
                            <label class="col-lg-3 form-control-label">Repository Url</label>
                            <div class="col-lg-9">
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="la la-github-square"></i>
                                        </span>
                                        <input type="text" class="form-control" id="githubrepo" name="githubrepo" placeholder="Enter Url">
                                    </div>
                                </div>
                            </div>
                    </div>

                    <div class="form-group row d-flex align-items-center mb-5">
                            <label class="col-lg-3 form-control-label">Status Project</label>
                            <div class="col-lg-9">
                                <input type="text" class="form-control" id="status_project" name="status_project" value="progress" readonly>
                            </div>
                    </div>

                    <button type="submit" class="btn btn-primary mr-1 mb-2" id="create-new-project"><i class="la la-send-o"></i>Launch your Project</button>
                </div>
                </form>
            </div>
        </div>
    </div>
</div>

<script>
    $('.dateproject').daterangepicker({
            singleDatePicker: true
	});
</script>

<script src="static/script/storage/myStorage.js"></script>
<script src="static/script/project.js" ></script>