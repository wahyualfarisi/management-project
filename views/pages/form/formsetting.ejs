<div class="row flex-row">
    <div class="col-xl-12">
        <!-- Form -->
        <div class="widget has-shadow">
            <div class="widget-header bordered no-actions d-flex align-items-center">
                <h4>Setting profile</h4>
            </div>
            <div class="widget-body">
                <form class="needs-validation" novalidate>
                    <div class="form-group row d-flex align-items-center mb-5">
                        <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Email</label>
                        <div class="col-lg-5">
                            <input type="text" class="form-control" placeholder="Enter your name" id="email_setting" disabled>
                        </div>
                    </div>
                    <div class="form-group row d-flex align-items-center mb-5">
                        <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Fullname</label>
                        <div class="col-lg-5">
                            <div class="input-group">
                                <input type="text" class="form-control" id="fullname" required>
                                <span class="input-group-addon addon-orange"><i class="la la-github-square" ></i> </span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row d-flex align-items-center mb-5">
                        <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">github username</label>
                        <div class="col-lg-5">
                            <div class="input-group">
                                <input type="text" class="form-control" id="githubusername" required>
                                <span class="input-group-addon addon-orange"><i class="la la-github-square" ></i> </span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row d-flex align-items-center mb-5">
                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Jobs</label>
                            <div class="col-lg-5">
                                <input type="text" class="form-control" id="jobs" name="jobs">
                            </div>
                        </div>                    
                    <div class="text-right">
                        <button class="btn btn-gradient-01" type="button" id="btn-save-change">Save Changes</button>
                        <a href="#/dashboard" class="btn btn-shadow" type="reset">Cancel</a>
                    </div>
                </form>
            </div>
        </div>
        <!-- End Form -->
    </div>
</div>
<!-- End Row -->

<div>
    <div class="widget has-shadow">
        <div class="widget-header bordered no-actions d-flex align-items-center">
            <h4>GITHUB REPOSITORY</h4>
        </div>
        <div class="widget-body" id="show-github">
            
        </div>
    </div>
</div>
